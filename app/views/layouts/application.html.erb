    <link rel="icon" type="image/png" href="<%= asset_path("favicon.png")%>" />
</head>
  <body class="<%= controller.controller_name %>">
    <main>
      <%= render  "layouts/topnav" %>
      <%= render "layouts/message"%>
      <%= yield %>
      <%= render  "layouts/footer" %>
    </main>
    <%= render  "layouts/nav" %>
  </body>
</html>
